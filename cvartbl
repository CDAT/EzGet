     +       vdimbdry(2,4,maxvars),  
     +       vdimcycl(4,maxvars), varnam(maxvars), filnam(maxvars),
     +       vartitle(maxvars), varunits(maxvars), vardate(maxvars),
     +       vartime(maxvars), varsourc(maxvars), vartype(maxvars),
     +       vdimname(4,maxvars), vdimdom(4,maxvars), vdimwt(4,maxvars)
