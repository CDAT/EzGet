
c   ***  cvartbl  ***

      integer ivarndim, iptdim, ivarlen, igeogin, igeogout, ishowin,
     +      ishowout, iregrid, iptdimrg, ivdimpln, nlatrgdv, nlonrgdv
      real vdimbdry, vdimcycl, dlatrgdv, alatrgdv, dlonrgdv, alonrgdv
      character varnam*64, filnam*120, vartitle*80, varunits*40,
     +       vardate*8, vartime*8, varsourc*120, vartype*8, 
     +       vdimname*16, vdimdom*8, vdimwt*8
      common /cvartbl/ ivarndim(maxvars), iptdim(4,maxvars),
     +       ivarlen(maxvars), igeogin(maxvars), igeogout(maxvars), 
     +       ishowin(maxvars), ishowout(maxvars), iregrid(maxvars), 
